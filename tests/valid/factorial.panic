_decl_ factorial _fn_ (n : i32) -> (i32 | ArithmeticOverflow | StackOverflow) {
    _if_ n < 0 {
        _return_ n;
    } _else_ _if_ n == 0 {
        _return_ 1;
    } _else_ {
        _return_ n * factorial((n - 1)?)?;
    }
}

